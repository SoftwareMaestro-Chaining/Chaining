<div id="modal_jupyter" class="modal modal-fixed-footer">
  <form class="col s12" action="/workspaces/<%=workspace.id%>/jupyters/" method="post" id="jupyter-form">
    <div class="modal-content">
      <div class="card-panel">
        <h4 class="header2">Create a new Jupyter</h4>
          <div class="row">

              <div class="row">
                <div class="input-field col s6">
                  <input id="jupyter_name" type="text" name="name">
                  <label for="jupyter_name">Name</label>
                </div>
                <div class="input-field col s6">
                  <input id="jupyter_description" type="text" name="description">
                  <label for="jupyter_description">Description</label>
                </div>
              </div>

              <div class="divider"></div>
              
              <div class="row">
                <div class="input-field col s6">
                  <input id="jupyter_endpoint" type="text" name="endpoint">
                  <label for="jupyter_endpoint">Endpoint</label>
                </div>
                <div class="input-field col s6">
                  <input id="jupyter_port" type="text" name="port">
                  <label for="jupyter_port">Port</label>
                </div>
              </div>
           </div>
        </div>
      </div>
  </form>

      <div class="modal-footer">
        <button href="#" class="waves-effect waves-red btn-flat modal-action modal-close" type="submit" id="jupyterFormSubmitBtn">Submit</button>
        <button class="waves-effect waves-green btn-flat modal-action modal-close">Cancel</button>
      </div>



    </div>
</div>


